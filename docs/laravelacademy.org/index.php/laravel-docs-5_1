                                                                public function get_recommend_product_list($query,$offset = 0, $limit = 20){        if(empty($query) || !is_array($query)){            return array();        }        if(ENVIRONMENT=='testing'){            $product_name = 'stage_product.product';        }else{            $product_name = 'prod_product.product';        }        $db = $this-&gt;load-&gt;database('ad_s', true);        foreach ($query as $key =&gt; $val) {            if (is_array($val)) {                $db-&gt;where_in($key, $val);            }            else {                $db-&gt;where($key, $val);            }        }        $db-&gt;select('DISTINCT commodity_id as product_id');        $db-&gt;from('recommend_son');        $db-&gt;join('commodity_recommendation', 'recommend_son.co_id = commodity_recommendation.id','left');        $db-&gt;join($product_name.' as product', 'product.id = commodity_id','left');        $db-&gt;where('product.is_onshelf', 1);        $db-&gt;order_by('sort','asc');        if($limit&gt;0){            $db-&gt;limit($limit,$offset);        }        $query = $db-&gt;get();        if ($query-&gt;num_rows() &gt; 0) {            return $query-&gt;result_array();        }        return array();    }è¯·é—®æ€ä¹ˆç”¨laravelæ”¹å†™ <img src="http://img.t.sinajs.cn/t35/style/images/common/face/ext/normal/9d/sada_org.gif" /> 
